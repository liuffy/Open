{
    "name": "Open",
    "version": "0.0.1",
    "manifest_version": 2,

		"icons": { 
			"48": "icons/icon48.png",
			// "16": "icons/icon16.png",
			"128": "icons/icon128.png" 
		},

		 "permissions": [
	    "geolocation",
	    "activeTab",
	    "tabs",
	    "https://ajax.googleapis.com/",
			"http://*/*",  
			"https://*/*"
  ],

    "browser_action": {
		"default_icon": { 
			// "16": "icons/icon16.png",
	    "48": "icons/icon48.png",
	 		 "128": "icons/icon128.png" 
	 		},
        "default_title": "Open",
        "default_popup": "popup.html"
    }

}